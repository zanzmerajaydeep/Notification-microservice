package com.notifier.model;

import org.springframework.data.mongodb.core.mapping.Document;
import org.springframework.data.mongodb.core.mapping.Field;
import org.springframework.data.mongodb.core.mapping.FieldType;

import jakarta.persistence.Id;

@Document("student")
public class Student {
	
	@Id
    @Field(name = "id", targetType = FieldType.INT64)
    private Long id;
	private String studentName;
	private int studentMark;
	private String studentEmail;
	private String studentMentorName;
	private String studentUniversity;
	private String studentContactno;
	public String getStudentName() {
		return studentName;
	}
	public void setStudentName(String studentName) {
		this.studentName = studentName;
	}
	public int getStudentMark() {
		return studentMark;
	}
	public void setStudentMark(int studentMark) {
		this.studentMark = studentMark;
	}
	public String getStudentEmail() {
		return studentEmail;
	}
	public void setStudentEmail(String studentEmail) {
		this.studentEmail = studentEmail;
	}
	public String getStudentMentorName() {
		return studentMentorName;
	}
	public void setStudentMentorName(String studentMentorName) {
		this.studentMentorName = studentMentorName;
	}
	public String getStudentUniversity() {
		return studentUniversity;
	}
	public void setStudentUniversity(String studentUniversity) {
		this.studentUniversity = studentUniversity;
	}
	public String getStudentContactno() {
		return studentContactno;
	}
	public void setStudentContactno(String studentContactno) {
		this.studentContactno = studentContactno;
	}
	
	
	public Long getId() {
		return id;
	}
	public Student(String studentName, int studentMark, String studentEmail, String studentMentorName,
			String studentUniversity, String studentContactno) {
		super();
		this.studentName = studentName;
		this.studentMark = studentMark;
		this.studentEmail = studentEmail;
		this.studentMentorName = studentMentorName;
		this.studentUniversity = studentUniversity;
		this.studentContactno = studentContactno;
	}
	public void setId(Long id) {
		this.id = id;
	}
	@Override
	public String toString() {
		return "Student [studentName=" + studentName + ", studentMark=" + studentMark + ", studentEmail=" + studentEmail
				+ ", studentMentorName=" + studentMentorName + ", studentUniversity=" + studentUniversity
				+ ", studentContactno=" + studentContactno + "]";
	}
	public Student() {
		super();
		// TODO Auto-generated constructor stub
	}
	
	
	
	
	
}
